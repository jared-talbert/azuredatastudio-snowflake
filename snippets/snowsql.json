{
	"Create a new Database": {
		"prefix": "snowCreateDatabase",
		"body": [
			"-- CREATE [ OR REPLACE ] [ TRANSIENT ] DATABASE [ IF NOT EXISTS ] '${1:DatabaseName}'",
			"CREATE DATABASE ${1:DatabaseName}"
		],
